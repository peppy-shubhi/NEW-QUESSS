#include <bits/stdc++.h>
using namespace std;

int main() {
	int t,i,sum,e,o;
	string s;
	cin>>t;
	while(t--)
	{
	    e=0;
	    o=0;
	    cin>>s;
	    for(i=0;i<s.length();i++)
	    {
	        
	        if(i%2==0&&s[i]=='1')
	        e+=1;
	        else if(i%2!=0&&s[i]=='1')
	        o+=2;
	    }
	    
	    if((e+o)%3==0)
	    cout<<"1"<<endl;
	    else cout<<"0"<<endl;
	}
	return 0;
}
